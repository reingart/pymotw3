msgid "``assert_()``"
msgstr ""

#: ../../source/porting_notes.rst:1166
msgid "``assertTrue()``"
msgstr ""

#: ../../source/porting_notes.rst:1167
msgid "``assertEquals()``"
msgstr ""

#: ../../source/porting_notes.rst:1168
msgid "``assertEqual()``"
msgstr ""

#: ../../source/porting_notes.rst:1169
msgid "``assertNotEquals()``"
msgstr ""

#: ../../source/porting_notes.rst:1170
msgid "``assertNotEqual()``"
msgstr ""

#: ../../source/porting_notes.rst:1171
msgid "``assertAlmostEquals()``"
msgstr ""

#: ../../source/porting_notes.rst:1172
msgid "``assertAlmostEqual()``"
msgstr ""

#: ../../source/porting_notes.rst:1173
msgid "``assertNotAlmostEquals()``"
msgstr ""

